{
    "name": "promises-aplus-tests",
    "description": "Compliance test suite for Promises/A+",
    "keywords": ["promises", "promises-aplus"],
    "version": "2.2.1",
    "implements": ["Promises/A+ 1.1.0"],
    "author": "Domenic Denicola <d@domenic.me> (https://domenic.me)",
    "license": "WTFPL",
    "repository": "promises-aplus/promises-tests",
    "main": "lib/programmaticRunner.js",
    "bin": "lib/cli.js",
    "files": [
        "lib/"
    ],
    "scripts": {
        "lint": "jshint lib",
        "test": "mocha",
        "prepublish": "node ./scripts/generateTestFiles.js",
        "update": "rm -rf package-lock.json node_modules npm-debug.log lib/testFiles.js; npm i && npm audit && npm run lint && npm run test && npm run prepublish"
    },
    "dependencies": {
        "mocha": "latest",
        "sinon": "latest",
        "underscore": "latest"
    },
    "devDependencies": {
        "jshint": "latest"
    },
    "browser": {
        "mocha": false
    }
}
